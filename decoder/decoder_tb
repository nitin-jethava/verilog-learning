#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_001C3B68 .scope module, "decoder_2to4_tb" "decoder_2to4_tb" 2 7;
 .timescale -9 -12;
v001C2D28_0 .var "a", 1 0;
v001CE418_0 .net "d", 3 0, v01023870_0; 1 drivers
S_001C2C48 .scope module, "uut" "decoder2to4" 2 14, 3 4, S_001C3B68;
 .timescale 0 0;
v001C2CD0_0 .net "data_in", 1 0, v001C2D28_0; 1 drivers
v01023870_0 .var "out", 3 0;
E_001C4A70 .event edge, v001C2CD0_0;
    .scope S_001C2C48;
T_0 ;
    %wait E_001C4A70;
    %load/v 8, v001C2CD0_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_0.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_0.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_0.2, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_0.3, 6;
    %set/v v01023870_0, 0, 4;
    %jmp T_0.5;
T_0.0 ;
    %movi 8, 1, 4;
    %set/v v01023870_0, 8, 4;
    %jmp T_0.5;
T_0.1 ;
    %movi 8, 2, 4;
    %set/v v01023870_0, 8, 4;
    %jmp T_0.5;
T_0.2 ;
    %movi 8, 4, 4;
    %set/v v01023870_0, 8, 4;
    %jmp T_0.5;
T_0.3 ;
    %movi 8, 8, 4;
    %set/v v01023870_0, 8, 4;
    %jmp T_0.5;
T_0.5 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_001C3B68;
T_1 ;
    %vpi_call 2 17 "$dumpfile", "decoder_wave.vcd";
    %vpi_call 2 18 "$dumpvars", 1'sb0, S_001C3B68;
    %vpi_call 2 20 "$monitor", "Time = %t | input = %b | output = %b", $time, v001C2D28_0, v001CE418_0;
    %set/v v001C2D28_0, 0, 2;
    %delay 10000, 0;
    %movi 8, 1, 2;
    %set/v v001C2D28_0, 8, 2;
    %delay 10000, 0;
    %movi 8, 2, 2;
    %set/v v001C2D28_0, 8, 2;
    %delay 10000, 0;
    %set/v v001C2D28_0, 1, 2;
    %delay 10000, 0;
    %vpi_call 2 29 "$finish";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "decoder_2to4_tb.v";
    "decoder_2to4.v";
