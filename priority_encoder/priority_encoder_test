#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_000F40C8 .scope module, "priority_encoder_tb" "priority_encoder_tb" 2 7;
 .timescale -9 -12;
v000F2D00_0 .var "data_in", 3 0;
v010438E8_0 .net "out", 1 0, v000F2C50_0; 1 drivers
v01043940_0 .net "valid", 0 0, v000F2CA8_0; 1 drivers
S_000F4150 .scope module, "uut" "priority_encoder" 2 14, 3 4, S_000F40C8;
 .timescale 0 0;
v000F8EC0_0 .net "data", 3 0, v000F2D00_0; 1 drivers
v000F2C50_0 .var "out", 1 0;
v000F2CA8_0 .var "valid", 0 0;
E_000F78E0 .event edge, v000F8EC0_0;
    .scope S_000F4150;
T_0 ;
    %wait E_000F78E0;
    %set/v v000F2CA8_0, 0, 1;
    %set/v v000F2C50_0, 0, 2;
    %ix/load 1, 3, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.0, 4;
    %load/x1p 8, v000F8EC0_0, 1;
    %jmp T_0.1;
T_0.0 ;
    %mov 8, 2, 1;
T_0.1 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_0.2, 8;
    %set/v v000F2C50_0, 1, 2;
    %set/v v000F2CA8_0, 1, 1;
    %jmp T_0.3;
T_0.2 ;
    %ix/load 1, 2, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.4, 4;
    %load/x1p 8, v000F8EC0_0, 1;
    %jmp T_0.5;
T_0.4 ;
    %mov 8, 2, 1;
T_0.5 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_0.6, 8;
    %movi 8, 2, 2;
    %set/v v000F2C50_0, 8, 2;
    %set/v v000F2CA8_0, 1, 1;
    %jmp T_0.7;
T_0.6 ;
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.8, 4;
    %load/x1p 8, v000F8EC0_0, 1;
    %jmp T_0.9;
T_0.8 ;
    %mov 8, 2, 1;
T_0.9 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_0.10, 8;
    %movi 8, 1, 2;
    %set/v v000F2C50_0, 8, 2;
    %set/v v000F2CA8_0, 1, 1;
    %jmp T_0.11;
T_0.10 ;
    %load/v 8, v000F8EC0_0, 1; Only need 1 of 4 bits
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_0.12, 8;
    %set/v v000F2C50_0, 0, 2;
    %set/v v000F2CA8_0, 1, 1;
T_0.12 ;
T_0.11 ;
T_0.7 ;
T_0.3 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_000F40C8;
T_1 ;
    %vpi_call 2 18 "$dumpfile", "priority_encoder.vcd";
    %vpi_call 2 19 "$dumpvars", 1'sb0, S_000F40C8;
    %vpi_call 2 21 "$monitor", "Time = %0t | data_in= %b | out = %b | valid = %b", $time, v000F2D00_0, v010438E8_0, v01043940_0;
    %set/v v000F2D00_0, 0, 4;
    %delay 10000, 0;
    %movi 8, 1, 4;
    %set/v v000F2D00_0, 8, 4;
    %delay 10000, 0;
    %movi 8, 2, 4;
    %set/v v000F2D00_0, 8, 4;
    %delay 10000, 0;
    %movi 8, 4, 4;
    %set/v v000F2D00_0, 8, 4;
    %delay 10000, 0;
    %movi 8, 8, 4;
    %set/v v000F2D00_0, 8, 4;
    %delay 10000, 0;
    %movi 8, 10, 4;
    %set/v v000F2D00_0, 8, 4;
    %delay 10000, 0;
    %movi 8, 7, 4;
    %set/v v000F2D00_0, 8, 4;
    %delay 10000, 0;
    %set/v v000F2D00_0, 1, 4;
    %delay 10000, 0;
    %vpi_call 2 37 "$finish";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "priority_encoder_tb.v";
    "priority_encoder.v";
