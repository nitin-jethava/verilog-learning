#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_008B2C50 .scope module, "binary_couter" "binary_couter" 2 12;
 .timescale 0 0;
v008B2CD8_0 .var "count", 2 0;
    .scope S_008B2C50;
T_0 ;
    %set/v v008B2CD8_0, 0, 3;
T_0.0 ;
    %load/v 8, v008B2CD8_0, 3;
    %mov 11, 0, 3;
   %cmpi/u 8, 8, 6;
    %jmp/0xz T_0.1, 5;
    %delay 10, 0;
    %vpi_call 2 17 "$display", "count: %b", v008B2CD8_0;
    %load/v 8, v008B2CD8_0, 3;
    %mov 11, 0, 29;
    %addi 8, 1, 32;
    %set/v v008B2CD8_0, 8, 3;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "binary_2bit_counter.v";
